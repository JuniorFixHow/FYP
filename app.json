{
  "expo": {
    "name": "FindMe",
    "slug": "FindMe",
    "version": "1.0.0",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#0EB4CB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-localization",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "9350f49b-277d-45ef-b5b6-038a5b55070e"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.juniorfixhow.FindMe",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon1.png",
        "backgroundColor": "#0EB4CB"
      }
    }
    
  },
  "name": "FindMe"
}
